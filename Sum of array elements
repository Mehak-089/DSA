// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
int  sum(int arr[],int n){
    int summ=0;
    for(int i=0;i<n;i++){
     summ=summ+arr[i];
    }
    return summ;
}
int main() {
        int n;
        cin>>n;
        int arr[n];
        for(int i=0;i<n;i++){
            cin>>arr[i];
        }
       int totalsum= sum(arr , n );
        // for(int i=0;i<n;i++){
        //     cout << arr[i]<<" ";
        cout<<totalsum;
return 0;
